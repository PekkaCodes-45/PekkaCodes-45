import {
  IsBoolean,
  IsNotEmpty,
  IsNumber,
  IsOptional,
  IsString,
} from 'class-validator'

export class MetadataCreateDto {
  @IsString()
  @IsOptional()
  keyword?: string

  @IsString()
  @IsOptional()
  documentId?: string

  @IsString()
  @IsOptional()
  dateCreated?: string

  @IsString()
  @IsOptional()
  dateDeleted?: string

  @IsString()
  @IsOptional()
  dateUpdated?: string
}

export class MetadataUpdateDto {
  @IsString()
  @IsOptional()
  keyword?: string

  @IsString()
  @IsOptional()
  documentId?: string

  @IsString()
  @IsOptional()
  dateCreated?: string

  @IsString()
  @IsOptional()
  dateDeleted?: string

  @IsString()
  @IsOptional()
  dateUpdated?: string
}
